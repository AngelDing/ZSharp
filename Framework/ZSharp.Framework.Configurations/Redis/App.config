<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="redisCacheClient"
			   type="ZSharp.Framework.Configurations.RedisCachingSectionHandler, ZSharp.Framework.Configurations" />
	</configSections>

  <appSettings>
    <add key="RedisConfigName" value="Common"/>
  </appSettings>
  
  <redisConfig>
    <hostGroup name="Common" allowAdmin="true" ssl="false" connectTimeout="5000" database="0">
      <hosts>
        <add ip="127.0.0.1" port="6379"/>
        <add ip="127.0.0.1" port="6380"/>
      </hosts>
    </hostGroup>
  </redisConfig>

</configuration> 